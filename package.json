{
  "name": "wall",
  "version": "0.0.0",
  "description": "Simple client-side SPA that displays a user-uploaded image on a wall of logos. ",
  "main": "src/index.js",
  "author": "Adam DÄ…browski",
  "license": "GPL",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.20.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators": "^6.13.0",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-duplicate-keys": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-parameters": "^6.21.0",
    "babel-plugin-transform-exponentiation-operator": "^6.8.0",
    "babel-plugin-transform-jsx": "^2.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "child-process": "^1.0.2",
    "eslint": "^3.12.2",
    "fs": "^0.0.1-security",
    "jsdom": "^9.9.1",
    "net": "^1.0.2",
    "virtual-dom": "^2.1.1",
    "webpack": "^1.14.0",
    "jsx-virtual-dom": "^0.0.0"
  },
  "dependencies": {
    "ramda": "^0.23.0"
  },
  "babel": {
    "presets": [
      "stage-0"
    ],
    "plugins": [
      "syntax-trailing-function-commas",
      "transform-async-to-generator",
      "transform-es2015-destructuring",
      "transform-es2015-parameters",
      "transform-es2015-duplicate-keys",
      "transform-es2015-modules-commonjs",
      "transform-exponentiation-operator",
      "transform-runtime",
      "transform-decorators",
      "syntax-jsx",
      ["transform-jsx", { "module": "jsx-to-virtual-dom" }]
    ],
    "env": {
      "test": {
        "plugins": [
          "rewire"
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "env": {
      "browser": true
    }
  },
  "scripts": {
    "build": "babel-node tools/run bundle"
  }
}
